{
  "name": "insomnia-website",
  "private": true,
  "version": "1.0.0",
  "description": "Website for https://insomnia.rest",
  "author": "Gregory Schier <gschier1990@gmail.com>",
  "licence": "MIT",
  "scripts": {
    "develop.yarn": "yarn run develop",
    "develop": "yarn run build:data && gatsby develop --port 8001",
    "build:data": "node ./scripts/fetch-metrics.js",
    "build": "yarn run build:data && gatsby build",
    "serve": "gatsby serve",
    "format-code": "prettier --write \"**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.246",
    "gatsby-image": "^1.0.46",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-feed": "^1.3.20",
    "gatsby-plugin-google-analytics": "^1.0.29",
    "gatsby-plugin-less": "^1.1.8",
    "gatsby-plugin-manifest": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sharp": "^1.6.42",
    "gatsby-source-filesystem": "^1.5.28",
    "gatsby-transformer-remark": "^1.7.39",
    "gatsby-transformer-sharp": "^1.6.22",
    "hkdf": "^0.0.2",
    "node-forge": "^0.7.5",
    "prop-types": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-waypoint": "^8.0.1",
    "srp-js": "^0.2.1"
  },
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "gray-matter": "^4.0.1",
    "husky": "^0.14.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0"
  }
}
